page {
  background: #f8f8f8;
  padding-bottom: 20rpx;
}

.user_head {
  /* background-image: url('https://a.mindamedia.com/bg.jpg'); */
  background: #fff;
  background-size: cover;
  height: 382rpx;
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
  overflow: hidden;
}

.user_head .vip-bg {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
}

.user_head .icon {
  position: absolute;
  width: 48rpx;
  height: 48rpx;
  top: 20rpx;
}

.user_head .msg {
  left: 20rpx;
}

.user_head .msg_tip {
  position: absolute;
  background: red;
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
  color: #fff;
  font-size: 12px;
  text-align: center;
  top: 20rpx;
  left: 45rpx;
}

.user_head .qrcode {
  position: absolute;
  right: 100rpx;
}

.user_head .set {
  position: absolute;
  right: 20rpx;
}

.user_head .user_info {
  color: #fff;
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  position: relative;
  z-index: 9;
}

.user_head .user_info .user_img {
  width: 134rpx;
  height: 134rpx;
  border: 4rpx solid #fff;
  border-radius: 50%;
  margin-bottom: 28rpx;
}

.user_head .user_info .edit {
  width: 30rpx;
  height: 30rpx;
}

.user_head .user_info .name {
  display: flex;
  align-items: center;
}

.user_head .user_info .name text {
  line-height: 0 !important;
}

.user_head .user_info .name image {
  width: 40rpx;
  margin-left: 10rpx;
  position: relative;
  top: 6rpx;
}

.user_body {
  margin-top: 16rpx;
  background: #fff;
}

.user_item {
  height: 108rpx;
  display: flex;
}

.user_item .icon {
  width: 100rpx;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.user_item .icon image {
  width: 42rpx;
  height: 42rpx;
}

.user_item .item {
  display: flex;
  align-items: center;
  border-bottom: 2rpx solid #e5e5e5;
  font-size: 30rpx;
  color: #333;
  flex: 1;
}

/* .user_body .item:last-child {
  border-bottom: none;
} */

.balance {
  background: #fff;
  display: flex;
  padding: 0 40rpx;
  height: 130rpx;
  justify-content: space-between;
}

.balance_money {
  display: flex;
  align-items: center;
  font-size: 30rpx;
  color: #333;
}

.balance_money .money {
  font-size: 40rpx;
  color: #1E1A19;
}

.balance_btn {
  display: flex;
  align-items: center;
}

.balance_btn button {
  color: #fff;
  border: 0;
  background: #1E1A19;
  border-radius: 50rpx;
}

