.create {
  width: 481rpx;
  height: 546rpx;
  margin-top: 102rpx;
}

.home_info {
  background: #fff;
  height: 190rpx;
  position: absolute;
  bottom: -100rpx;
  width: 680rpx;
  left: 0;
  right: 0;
  margin: auto;
  box-shadow: 0rpx 8rpx 20rpx 0rpx rgba(8, 8, 8, 0.09);
  display: flex;
  border-radius: 14rpx;
}

.home_info view {
  flex-grow: 1;
  padding: 20rpx;
}

.home_info text {
  font-size: 32rpx;
  color: #101010;
}

.home_info .money {
  line-height: 35px;
  color: #1E1A19;
  font-size: 48rpx;
  display: flex;
  align-items: center;
  padding: 20rpx 0;justify-content:center;
}

.home_info .icon_money {
  width: 44rpx;
  height: 44rpx;
  opacity: 0;
}

.my_create {
  color: #454545;
  font-size: 32rpx;
  text-align: center;
  padding-top: 40rpx;
  position: relative;
}

.my_create view::before {
  position: absolute;
  content: '';
  background: rgba(0, 0, 0, 0.2);
  height: 2rpx;
  top: 58rpx;
  width: 24%;
  left: 40rpx;
}

.my_create view:after {
  position: absolute;
  content: '';
  background: rgba(0, 0, 0, 0.2);
  height: 2rpx;
  top: 58rpx;
  width: 24%;
  right: 40rpx;
}

.distribution_box {
  padding: 40rpx;
}

.distribution_box .distribution_list {
  height: 212rpx;
  background: #fff;
  padding: 20rpx;
  display: flex;
  /* align-items: center; */
  margin-bottom: 30rpx;
  border-radius: 10rpx;
  overflow: hidden;
}

.distribution_box .distribution_list:last-child {
  border-bottom: 0;
}

.distribution_box .distribution_list .distribution_img {
  width: 160rpx !important;
  height: 160rpx !important;
  border-radius: 50%;
  /* background: pink; */
}

.distribution_box .distribution_list .distribution_info {
  /* width: 75%; */
 flex: 1;
  padding-left: 20rpx;
  overflow: hidden;
}

.distribution_box .distribution_list .distribution_info .distribution_title {
  font-size: 36rpx;
  color: #101010;
  line-height: 52rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  width: 100%;
  display: inline-block;
}

.distribution_box .distribution_list .distribution_info .profit {
  font-size: 24rpx;
  color: #bbb;
  padding-top: 10rpx;
  border-bottom: 2rpx solid rgba(0, 0, 0, 0.1);
  padding-bottom: 10rpx;
}

.distribution_box .distribution_list .distribution_info .money {
  font-size: 36rpx;
  color: #1E1A19;
}

.distribution_box .distribution_list .distribution_info .distribution_value {
  display: flex;
  padding-top: 10rpx;
}

.distribution_box .distribution_list .distribution_info .distribution_value view {
  flex-grow: 1;
  flex: 1;
  font-size: 24rpx;
  color: #bbb;
}
/* vip */

.vip-panel {
  background: #fff;
  width: 680rpx;
  margin: 0 auto;
  border-radius: 14rpx;
  display: flex;
  box-shadow: 0rpx 8rpx 20rpx 0rpx rgba(8, 8, 8, 0.09);
  margin-top: 140rpx;
  margin-bottom: 40rpx;
}

.vip-item {
  width: 50%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  height: 194rpx;
  position: relative;
}

.vip-item:nth-of-type(2n):before {
  display: block;
  position: absolute;
  left: -1.5rpx;
  top: 50%;
  content: '';
  height: 86rpx;
  margin-top: -43rpx;
  background: #e5e5e5;
  width: 3rpx;
}

.vip-item image {
  width: 68rpx;
}

.vip-item text {
  font-size: 32rpx;
  color: #333;
  text-align: center;
} 

.contact{
  width: 100%;height: 108rpx;opacity: 0;position: absolute;z-index: 100;
}

.time{
  width: 230rpx;
  position: absolute;
  right: 0;
  top: 10rpx;
  color: #fff;
  font-size: 20rpx;
  text-align: right;
  padding-right: 20rpx;
}
