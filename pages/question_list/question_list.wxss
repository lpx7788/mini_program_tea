page {
  background: #f8f8f8;
  font-size: 28rpx;
  color: #333;
}

.list {
  background: #fff;
  margin: 20rpx 0;
}

.list .item {
  display: flex;
  justify-content: space-between;
  height: 110rpx;
  border-bottom: 1px solid #e5e5e5;
  padding: 0 34rpx;
  align-items: center;
}

.list .item:last-child {
  border-bottom: none;
}